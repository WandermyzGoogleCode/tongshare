<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>

<%= devise_error_messages_translated! %>

<%= label_for_login :employee_no%><br/>
<%= text_field_for_login :employee_no%><br/>

<%= f.label :password, '“同享日程”密码'%><br/>
<%= f.password_field :password, :size => nil%><br/>

<%= f.label :password_confirmation, '确认密码', :class=>"title"  %><br/>
<%= f.password_field :password_confirmation, :class=>"textvalue", :size => nil %><br/>

<label for="infopass">清华大学Info密码</label><br/>
<%= password_field_tag "infopass", nil%><br/>

<%= f.submit "注册"%>

<% end %>